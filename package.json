{
  "name": "typescript-restful-starter",
  "version": "0.1.0",
  "description": "kit starter node application",
  "main": "server.js",
  "scripts": {
    "start": "npm run build && NODE_ENV=PRODUCTION node dist/Index.js",
    "dev": "NODE_ENV=DEV ./node_modules/.bin/nodemon --watch '**/*.ts' --exec './node_modules/.bin/ts-node' Index.ts",
    "test": "./node_modules/.bin/_mocha --timeout 20000 ./node_modules/.bin/_ts-node test/TestRouter.ts",
    "build": "npm run clean && ./node_modules/.bin/tsc --outDir dist",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "node",
    "express",
    "typeorm",
    "typescript",
    "tslint",
    "JWT",
    "E2015"
  ],
  "author": "Hector Riquelme",
  "license": "MIT",
  "dependencies": {
    "@types/express": "^4.0.35",
    "@types/jsonwebtoken": "^7.2.0",
    "@types/method-override": "0.0.31",
    "@types/node": "^9.4.0",
    "body-parser": "^1.17.1",
    "cluster": "^0.7.7",
    "cors": "^2.8.3",
    "express": "^4.15.2",
    "express-jwt": "^5.3.0",
    "jsonwebtoken": "^8.1.1",
    "method-override": "^2.3.8",
    "morgan": "^1.9.0",
    "mysql": "^2.13.0",
    "os": "^0.1.1",
    "path": "^0.12.7",
    "superagent": "^3.5.2",
    "token-extractor": "^0.1.6",
    "ts-node": "^4.1.0",
    "typeorm": "^0.1.2",
    "typescript": "^2.3.3"
  },
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/mocha": "^2.2.41",
    "@types/superagent": "^3.5.1",
    "chai": "^4.0.2",
    "mocha": "^5.0.0",
    "nodemon": "^1.11.0"
  }
}
